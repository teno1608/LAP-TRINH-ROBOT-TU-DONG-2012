.................... void lcd_putnum(int32 so,INT8 x,int8 y) 
.................... { 
....................    INT32 a, b, c, d, e; 
....................    a = so / 10000; 
....................    b = (so - 10000 * a) / 1000; 
....................    c = (so - 10000 * a - 1000 * b) / 100; 
....................    d = (so - 10000 * a - 1000 * b - 100 * c) / 10; 
....................    e = so - 10000 * a - 1000 * b - 100 * c - 10 * d; 
....................    lcd_gotoxy (x, y); 
....................    lcd_putc (a + 48); 
....................    lcd_putc (b + 48); 
....................    lcd_putc (c + 48); 
....................    lcd_putc (d + 48); 
....................    lcd_putc (e + 48); 
.................... } 
